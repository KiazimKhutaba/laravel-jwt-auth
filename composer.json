{
    "name": "devkit2026/laravel-jwt-auth",
    "description": "A standalone JWT authentication module for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Devkit2026",
            "email": "info@devkit2026.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "firebase/php-jwt": "^6.10",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/auth": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0",
        "illuminate/mail": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Devkit2026\\JwtAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devkit2026\\JwtAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devkit2026\\JwtAuth\\JwtAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}